generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String   @id @default(cuid())
  telegramUserId String   @unique
  username       String?
  firstName      String?
  lastName       String?
  photoUrl       String?
  nickname       String?
  phone          String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
